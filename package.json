{
  "name": "erela.js",
  "version": "1.1.8",
  "description": "An easy-to-use Lavalink client for NodeJS.",
  "main": "dist/index.js",
  "types": "typings/index.d.ts",
  "repository": "github:WarHammer414/erela.js",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "types": "dts-bundle --removeSource --name erela.js --main ./dist --out ../typings/index.d.ts --baseDir ./dist"
  },
  "keywords": [
    "lavalink",
    "lavalink client",
    "music"
  ],
  "author": "WarHammer414 (https://github.com/WarHammer414)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^14.0.1",
    "@types/ws": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "dts-bundle": "^0.7.3",
    "eslint": "^7.0.0",
    "tslint": "^5.20.1",
    "typedoc": "^0.15.8",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "@discordjs/collection": "^0.1.5",
    "axios": "^0.19.2",
    "ws": "^7.2.1"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ]
  }
}
